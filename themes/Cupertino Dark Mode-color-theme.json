{
  "type": "dark",
  "colors": {
    "editor.background": "#000000",
    "editor.foreground": "#FFFFFF",
    "editorCursor.foreground": "#FFFFFF",
    "editor.lineHighlightBackground": "#1A1A1A",
    "editorLineNumber.foreground": "#B0B0B0",
    "editor.selectionBackground": "#2E2E2E",
    "editor.selectionHighlightBackground": "#1A1A1A80",
    "editor.wordHighlightBackground": "#2E2E2E80",
    "editorBracketMatch.border": "#FFFFFF",
    "activityBar.background": "#1A1A1A",
    "activityBar.foreground": "#FFFFFF",
    "activityBarBadge.background": "#FFFFFF",
    "activityBarBadge.foreground": "#000000",
    "sideBar.background": "#1A1A1A",
    "sideBar.foreground": "#B0B0B0",
    "sideBarTitle.foreground": "#FFFFFF",
    "statusBar.background": "#000000",
    "statusBar.foreground": "#FFFFFF",
    "statusBar.debuggingBackground": "#2E2E2E",
    "statusBar.debuggingForeground": "#FFFFFF",
    "terminal.background": "#000000",
    "terminal.foreground": "#FFFFFF",
    "terminalCursor.background": "#FFFFFF",
    "terminalCursor.foreground": "#FFFFFF",
    "tab.activeBackground": "#1A1A1A",
    "tab.inactiveBackground": "#000000",
    "tab.activeForeground": "#FFFFFF",
    "tab.inactiveForeground": "#B0B0B0",
    "button.background": "#2E2E2E",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#1A1A1A",
    "dropdown.background": "#1A1A1A",
    "dropdown.foreground": "#FFFFFF",
    "input.background": "#2E2E2E",
    "input.foreground": "#FFFFFF",
    "scrollbarSlider.background": "#1A1A1A",
    "scrollbarSlider.hoverBackground": "#2E2E2E"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#B0B0B0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#FFFFFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#B0B0B0"
      }
    },
    {
      "scope": "function",
      "settings": {
        "foreground": "#FFFFFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#B0B0B0"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": "meta.import",
      "settings": {
        "foreground": "#B0B0B0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        
      }
    }
  ]
}